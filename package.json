    "@rneui/themed": "^4.0.0-rc.8",
    "dotenv": "^16.4.5",
    "expo": "~50.0.7",
    "expo-camera": "~14.1.2",
    "expo-clipboard": "^5.0.1",
    "expo-font": "^11.10.3",
    "expo-image-picker": "^14.7.1",
    "expo-linking": "~6.2.2",
    "expo-location": "~16.5.5",
    "expo-router": "~3.4.7",
